<div>
    <ul bs-list="bordered swipe">
        {{#each page.urls}}
        <li>
            <p>{{this}}</p>
            <button bs-remove><i bs-icon="bin"></i></button>
            <span bs-multi="right">
                <a href="#" bs-button>new tab</a>
                <a href="#" bs-button>sync all</a>
            </span>
        </li>
